// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./database.db"
}

model TCPPacket {
  id        Int      @id @default(autoincrement())
  timestamp DateTime
  source    String
  destination String
  length    Int
  data      String
}